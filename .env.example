# Copy to .env and fill in values.
# Used by: docker compose -f infra/docker-compose.dev.yml up (compose reads from repo root .env)

DATABASE_URL=postgresql://postgres:postgres@localhost:5432/makemyownmodel
REDIS_URL=redis://localhost:6379
QDRANT_URL=http://localhost:6333

# Required: 32-byte key as 64-char hex (e.g. openssl rand -hex 32)
APP_ENCRYPTION_KEY=

# NextAuth
NEXTAUTH_SECRET=
NEXTAUTH_URL=http://localhost:3000

# Optional (when using --profile ollama)
# OLLAMA_BASE_URL=http://localhost:11434
