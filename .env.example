# Copy to .env at repo root. Compose reads this for web/worker.
# Required for: docker compose -f infra/docker-compose.dev.yml up

# Database (compose overrides host to "postgres" for containers)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/makemyownmodel

# Redis
REDIS_URL=redis://localhost:6379

# Qdrant
QDRANT_URL=http://localhost:6333

# Required: 32-byte key as 64-char hex (e.g. openssl rand -hex 32)
APP_ENCRYPTION_KEY=

# NextAuth
NEXTAUTH_SECRET=
NEXTAUTH_URL=http://localhost:3000

# Optional (with --profile ollama)
# OLLAMA_BASE_URL=http://localhost:11434
