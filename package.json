{
  "name": "makemyownmodel",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter web dev",
    "build": "pnpm --filter web build",
    "test": "pnpm -r test",
    "lint": "pnpm run format:check && node scripts/run-next-lint.cjs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "pnpm --filter web exec prisma generate",
    "db:migrate": "pnpm --filter web exec prisma migrate deploy",
    "db:seed": "pnpm --filter web exec prisma db seed",
    "db:push": "pnpm --filter web exec prisma db push"
  },
  "devDependencies": {
    "next": "14.2.15",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "eslint": "^8.57.0"
    }
  }
}
